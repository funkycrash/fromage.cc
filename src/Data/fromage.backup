        "region": "Valais",
        "pays": "Suisse",
        "lait": "vache",
        "appellation": "AOP",
        "dateAppellation": "2003",
        "debut": 11,
        "fin": 4,
        "lien": "https://www.inao.gouv.fr/Nos-actualites/La-Raclette-de-Savoie-obtient-l-IGP",
        "photo": {
